{
  "name": "uistd/mysql",
  "description": "UI Service 简易Mysql数据库操作组件。",
  "keywords": [
    "ui service",
    "uistd",
    "mysql"
  ],
  "type": "composer",
  "authors": [
    {
      "name": "huangshunzhao",
      "email": "huangshunzhao@wanda.cn",
      "role": "Founder"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-mysqli": "*",
    "uistd/event": "3.*",
    "uistd/common": "3.*",
    "uistd/logger": "2.*",
    "uistd/console": "2.*"
  },
  "autoload": {
    "psr-4": {
      "UiStd\\Mysql\\": "src"
    }
  },
  "minimum-stability": "stable",
  "archive": {
    "exclude": [
      "test",
      ".gitignore"
    ]
  },
  "config": {
    "secure-http": false
  }
}