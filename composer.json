{
  "name": "ffan/php-mysql",
  "description": "FFan mysql",
  "keywords": [
    "ffan",
    "php",
    "mysql"
  ],
  "type": "composer",
  "authors": [
    {
      "name": "huangshunzhao",
      "email": "huangshunzhao@wanda.cn",
      "role": "Founder"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-mysqli": "*",
    "ffan/php-utils": "*",
    "ffan/php-logger": "*",
    "ffan/php-event": "*"
  },
  "autoload": {
    "psr-4": {
      "ffan\\php\\mysql\\": ""
    }
  },
  "minimum-stability": "dev",
  "archive": {
    "exclude": [
      "test",
      ".gitignore"
    ]
  },
  "config": {
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/private/"
    },
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/packagist/"
    },
    {
      "packagist": false
    }
  ]
}